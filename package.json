  "main": "dist/index.js",
  "jsnext:main": "dist/es2015/wsopcua/index.js",
  "es2015": "dist/es2015/wsopcua/index.js",
  "module": "dist/es2015/wsopcua/index.js",
  "dependencies": {
    "async": "^3.2.0",
    "loglevel": "^1.7.1",
    "setimmediate": "^1.0.5"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/loglevel": "^1.6.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "jasmine-ws": "~0.3.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-source-map-support": "^1.4.0",
    "karma-typescript": "^5.2.0",
    "karma-typescript-es6-transform": "^5.2.0",
    "start-server-and-test": "^1.11.7",
    "ts-loader": "^8.0.14",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "wsopcua-test-server": "^1.3.0"
  },
  "scripts": {
    "start": "webpack --watch --config webpack.dev.js",
    "startES2015": "tsc -p ./tsconfig.json --module es2015 --outDir ./dist/es2015 --watch",
    "build": "webpack --config webpack.dev.js && webpack --config webpack.mod.js && npm run build-es2015",
    "build-es2015": "tsc -p ./tsconfig.json --module es2015 --outDir ./dist/es2015",
    "build-examples": "webpack --config webpack.example.js",
    "test": "karma start",
    "test-server": "wsopcua-test-server",
    "e2e:run-headless": "karma start ./karma.e2e.conf.js --single-run --browsers ChromeHeadless",
    "e2e:start": "karma start ./karma.e2e.conf.js",
    "test-e2e-headless": "start-server-and-test test-server tcp:4444 e2e:run-headless",
    "start-e2e": "start-server-and-test test-server tcp:4444 e2e:start"
  },
  "files": [
    "documentation/**/*",
    "dist/es2015/**/*",
    "!dist/es2015/**/*.spec.*",
    "!dist/es2015/**/*.e2e-spec.*",
    "src/schema_parser/dist/wsopcua-schema-gen.js",
    "src/schema_parser/schemas/**/*"
  ]
}

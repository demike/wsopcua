  "dependencies": {
    "async-es": "^2.6.2",
    "delayed": "^1.0.1",
    "loglevel": "^1.6.1",
    "setimmediate": "^1.0.5"
  },
  "devDependencies": {
    "@types/async": "^2.4.0",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/loglevel": "^1.5.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-ws": "~0.3.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-source-map-support": "^1.4.0",
    "karma-typescript": "^4.0.0",
    "karma-typescript-es6-transform": "^4.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "scripts": {
    "start": "webpack --watch --config webpack.dev.js",
    "startES2015": "tsc -p ./tsconfig.json --module es2015 --outDir ./dist/es2015 --watch",
    "generate-typings": "tsc -d",
    "bundle-typings": "dts-bundle --name pts --outputAsModuleFolder true --main dist/files/**/*.d.ts --out ../pts.d.ts",
    "typings": "npm run generate-typings && npm run bundle-typings",
    "build": "webpack --config webpack.dev.js && webpack --config webpack.mod.js && npm run build-es2015",
    "build-es2015": "tsc -p ./tsconfig.json --module es2015 --outDir ./dist/es2015",
    "build-examples": "webpack --config webpack.example.js",
    "test": "karma start",
    "test:auto": "mocha --opts mocha.opts --watch",
    "docs": "typedoc --json docs/json/docs.json && python parse.py"
  }
}
